<h1>Show Url</h1>

<ul>

  <li>
    <strong>Long url:</strong>
    <%= @url.long_url %>
  </li>

  <li>
    <strong>Slug:</strong>
    <%= @url.slug %>
  </li>

  <li>
    <strong>Clicks:</strong>
    <%= @url.clicks %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.url_path(@conn, :edit, @url) %></span>
<span><%= link "Back", to: Routes.url_path(@conn, :index) %></span>
